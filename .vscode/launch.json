  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "babel",
      "program": "",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/babel-node",
      "sourceMaps": true,
      "env": {
        "BABEL_ENV": "debug"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "NPM",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "test"
      ],
      "sourceMaps": true,
      "env": {
        "BABEL_ENV": "debug"
      },
      "port": 9229
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
        "test/findSubstring.spec.js",
        "--require",
        "@babel/register",
        "--timeout",
        "99999999"
      ],
      "cwd": "${workspaceRoot}",
      "sourceMaps": true,
      "env": {
        "BABEL_ENV": "debug"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "leetcode",
      "program": "${workspaceFolder}/src/quickSort.js"
    }
  ]
}
